#include "pch.h"
#include<iostream>
using namespace std;

int main()
{
	int x, y;
	cout << "Please enter 2 integers of x and y \n ";
	cin >> x >> y;
	cout << "you entered the value of x is " << x << endl;
	cout << "you entered the value of y is " << y << endl;
	swap(x, y);
	cout << "The value of x after swapping is " << x << endl;
	cout << "The value of y after swapping is " << y <<endl;
	return 0;
}

void swap(int x, int y)
{
	cout << "you entered the value of x is " << x << endl;
	cout << "you entered the value of y is " << y << endl;
	int temp = x;
	x = y;
	y = temp;
	cout << "\n After swap the value of x now is: " << x << endl;
	cout << "\n After swap the value of y now is: " << y << endl;

}
